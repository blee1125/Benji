{
  "manifest_version": 3,
  "name": "Pro Gamer Excuse Generator",
  "version": "1.0",
  "description": "Generates a random excuse when you rage-mash keys.",
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Pro Gamer Excuse Generator"
  },
  "background": {
    "service_worker": "merged_script.js"
  }
}